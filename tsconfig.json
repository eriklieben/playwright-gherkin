{
	"compilerOptions": {
		"lib": ["es2022"],
		"module": "commonjs",
		"target": "es2022",
	
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
	
		"outDir": "dist",
		"declaration": true
	  },
	"compileOnSave": true,
	"include": ["src/**/*"],
	"exclude": ["node_modules", ".vscode"]
  }
  